{
  "name": "ngx-pattern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-pkg": "ng build --prod ngx-pattern && npm run copy-files",
    "copy-files": "cp README.md dist/ngx-pattern/README.md && cp LICENSE dist/ngx-pattern/LICENSE",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "publish:latest": "npm run build-pkg && npm publish dist/ngx-pattern",
    "publish:v1": "npm run build-pkg && npm publish dist/ngx-pattern --tag v1-stable",
    "dist-tags": "npm dist-tag ls",
    "gh-pages": "ng build ngx-pattern-demo"
  },
  "dependencies": {
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "rxjs": "~6.6.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^10.1.1",
    "protractor": "~5.4.0",
    "ts-node": "^9.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  }
}
