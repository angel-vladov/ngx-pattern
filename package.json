{
  "name": "ngx-pattern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-pkg": "ng build --configuration production ngx-pattern && npm run copy-files",
    "copy-files": "cp README.md dist/ngx-pattern/README.md && cp LICENSE dist/ngx-pattern/LICENSE",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "publish:latest": "npm run build-pkg && npm publish dist/ngx-pattern",
    "publish:v1": "npm run build-pkg && npm publish dist/ngx-pattern --tag v1-stable",
    "publish:v2": "npm run build-pkg && npm publish dist/ngx-pattern --tag v2-stable",
    "dist-tags": "npm dist-tag ls",
    "gh-pages": "ng build ngx-pattern-demo"
  },
  "dependencies": {
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
